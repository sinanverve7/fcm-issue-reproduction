{
  "appId": "com.example.app",
  "appName": "capacitor-app",
  "bundledWebRuntime": false,
  "webDir": "dist",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "FirebaseAuthentication": {
      "skipNativeAuth": false,
      "providers": ["phone"]
    },
    "FirebaseMessaging": {
      "presentationOptions": []
    }
  },
  "ios": {
    "cordovaLinkerFlags": ["-ObjC"]
  }
}